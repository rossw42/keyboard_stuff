{
  "name": "ergogen-dxf-viewer",
  "displayName": "Ergogen DXF Viewer (Self-Contained)",
  "description": "Self-contained DXF viewer for Ergogen keyboard design workflow",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "*"
  ],
  "main": "./self-contained-extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ergogen-dxf-viewer.test",
        "title": "Test Extension",
        "category": "Ergogen"
      },
      {
        "command": "ergogen-dxf-viewer.runErgogen",
        "title": "Run Ergogen",
        "category": "Ergogen"
      },
      {
        "command": "ergogen-dxf-viewer.openViewer",
        "title": "Open DXF Viewer",
        "category": "Ergogen"
      }
    ],
    "keybindings": [
      {
        "command": "ergogen-dxf-viewer.runErgogen",
        "key": "ctrl+shift+e",
        "when": "resourceExtname == .yaml"
      }
    ]
  }
}